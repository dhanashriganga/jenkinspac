pipeline {
 agent any
 stages {
  stage("working with conditions"){
   steps {
    script {
    val1=input message: 'Enter val1 value', parameters: [string(defaultValue: '10', name: 'val1', trim: true)]
    val12=input message: 'Enter val2 value', parameters: [string(defaultValue: '20', name: 'val2', trim: true)]
    if (val1.toInteger() > val2.toInteger()) {
     println "val1 is big ${val1}"
    }
    else {
     println "val2 is big ${val2}"
     }
    }
   }
  }
 }
}
