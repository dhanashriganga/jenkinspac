pipeline {
 agent any
 stages {
  stage("working with loops"){
   steps {
    script {
     mydata = [10,20,30]
     for(i in mydata) {
      println "my list values are ${i}"
     }
     for(j=1;j<=10;j++)
     {
      println "my j value is ${j}"
     }
     k=1
     while(k<=10){
      println "my k value is ${k}"
      i++
     }
    }
   }
  }
 }
}
